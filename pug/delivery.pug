                .serv-icon__img
                  img(src="./img/services/serv-icon-3.svg", alt="")
                .serv-icon__text От идеи до сдачи в эксплуатацию

          section.section
            h2.h2_center Транспортные компании
            .transport-companies
              - for (var a = 1; a <= 5; a++)
                .transport-company
                  img(src="./img/services/transport-companies/" + a + ".jpg", alt="")
    
    section.section.blocks
      .wrapper
        .block.grid
          .col.col--sm-5
            .block__plate примеры работ
            h2.block__title Отгрузка двух паровых котлов (парогенераторов)
            p.block__subtitle ECO-PAR-1500, деаэратора ДА 5/4, Краснодарский край
          .col.col--sm-1
          .col.col--sm-6
            a(href="").block__img.block__img_video
              img(src="./img/services/block-delivery-1.jpg", alt="")
        .block.grid
          .col.col--sm-6
            a(href="").block__img.block__img_video
              img(src="./img/services/block-delivery-2.jpg", alt="")
          .col.col--sm-1
          .col.col--sm-5
            .block__plate примеры работ
            h2.block__title Отгрузка блочно-модульной котельной
            p.block__subtitle Блочно-модульная котельная ECO-PAR-1000-D. Парогенератор для бетонного завода.

